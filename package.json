{
    "name": "selenium-mocha",
    "version": "1.0.0",
    "description": "selenium webdriver test using mocha with mochawesome reports",
    "scripts": {
      "test": "mocha tests/test_sesi9.js --recursive --timeout 60000",
      "test-parallel": "mocha tests --parallel --recursive --timeout 60000",
      "test-grep": "mocha tests/test_sesi9.js --grep 'A-Z' --recursive --timeout 60000",
      "jalanin-mochawesome": "mocha tests/test_sesi9.js --recursive --timeout 60000 --reporter mochawesome",
      "jalanin-simple-html": "mocha tests --recursive --timeout 60000 --reporter mocha-simple-html-reporter --reporter-options output=report.html"
    },
    "dependencies": {
      "selenium-webdriver": "^4.17.0"
    },
    "devDependencies": {
      "mocha": "^10.4.0",
      "mochawesome": "^7.1.3",
      "mocha-simple-html-reporter": "^2.0.0"
    }
  }